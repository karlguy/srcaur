#: srcpac:36
msgid "usage: %s <operation> [...]"
msgstr "folosire: %s <operație: [...]"

#: srcpac:37
msgid "operations:"
msgstr "operații:"

#: srcpac:38
msgid "        %s {-h --help}"
msgstr "        %s {-h --help}"

#: srcpac:39
msgid "        %s {-V --version}"
msgstr "        %s {-V --version}"

#: srcpac:40
msgid "        %s {-Q --query}   [options] [<package(s)>]"
msgstr "        %s {-Q --query}   [opțiuni] [<pachete(s)>]"

#: srcpac:41
msgid "        %s {-R --remove}  [options] <package(s)>"
msgstr "        %s {-R --remove}  [opțiuni] <pachete(s)>"

#: srcpac:42
msgid "        %s {-S --sync}    [options] [package(s)]"
msgstr "        %s {-S --sync}    [opțiuni] [pachete(s)]"

#: srcpac:43
msgid "%s options are based on pacman, so check the pacman man page"
msgstr "opțiunile %s sunt bazate pe pacman, deci consultați manualul pacman"

#: srcpac:44
msgid "%s adds some option to -S:"
msgstr "%s adaugă opțiuni la -S:"

#: srcpac:45
msgid "  -b, --build    builds the targets from source"
msgstr "  -b, --build    compilează ținta din codul sursă"

#: srcpac:46
msgid "  -bb            builds targets and their dependencies from source"
msgstr "  -bb\t\t compilează țintele și dependențele lor din codul sursă"

#: srcpac:47
msgid ""
"  -bbb           builds targets, dependencies and their makedeps from source"
msgstr ""
"  -bbb\t\tcompilează țintele, dependențele și dependențele make din codul "
"sursă"

#: srcpac:48
msgid "  -m, --makedeps remove the makedepends after the build"
msgstr "  -m, --makedeps șterge dependențele make după compilare"

#: srcpac:49
msgid ""
"  -o, --onlyconf applies the changes and displays the PKGBUILD without "
"building"
msgstr ""
"  -o, --onlyconf aplică schimbările și afișează fișierul PKGBUILD fără "
"compilare"

#: srcpac:50
msgid "%s adds some option to -Q:"
msgstr "%s adaugă unele opțiuni la -Q:"

#: srcpac:51
msgid "  -b, --build    show all packages built from source"
msgstr "  -b, --build\t arată toate pachetele compilate din cod sursă"

#: srcpac:52
msgid "%s adds some option to -R:"
msgstr "%s adaugă unele opțiuni la -R:"

#: srcpac:53
msgid "  -b, --build    remove build-flag only, not the package"
msgstr "  -b, --build\t înlătură doar indicatorul de compilare, nu și pachetul"

#: srcpac:54
msgid "  -o, --onlyconf remove the patchfile only"
msgstr "  -o, --onlyconf înlătură doar fișierul patch"

#: srcpac:139
msgid "Error: Could not find %s under %s"
msgstr "Eroare: Nu se poate găsi %s sub %s"

#: srcpac:148
msgid "Starting ABS sync..."
msgstr "Începe sincronizarea ABS..."

#: srcpac:150 srcpac:406
msgid "done"
msgstr "realizat"

#: srcpac:404
msgid "removing source reference %s..."
msgstr "se înlătură referința sursă %s..."

#: srcpac:434
msgid "Error: You need to use sudo or to be root"
msgstr "Eroare: Trebuie să folosiți sudo sau să fiți root"

#: srcpac:439
msgid "Error: The ABSROOT environment variable is not defined"
msgstr "Eroare: Variabila de mediu ABSROOT nu este definită"

#: srcpac:481
msgid "Source         : "
msgstr "Sursa\t\t:"

#: srcpac:483 srcpac:489
msgid "Yes"
msgstr "Da"

#: srcpac:485 srcpac:491
msgid "No"
msgstr "Nu"

#: srcpac:487
msgid "Patchfile      : "
msgstr "Fișier patch\t:"

#: srcpac:496
msgid " [source]"
msgstr " [sursă]"

#: srcpac:580
msgid "Source Targets:"
msgstr "Țintele sursă"

#: srcpac:586
msgid "Proceed? [Y/n]"
msgstr "Continuați? [Y/n]"

#: srcpac:588
msgid "y"
msgstr "y"

#: srcpac:588
msgid "Y"
msgstr "Y"

#: srcpac:599
msgid "Build failed for:%s"
msgstr "Compilarea a eșuat pentru:%s"


