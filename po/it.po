#: srcpac:36
msgid "usage: %s <operation> [...]"
msgstr "so: %s <operazioni> [...]"

#: srcpac:37
msgid "operations:"
msgstr "operazioni:"

#: srcpac:38
msgid "        %s {-h --help}"
msgstr "        %s {-h --help}"

#: srcpac:39
msgid "        %s {-V --version}"
msgstr "        %s {-V --version}"

#: srcpac:40
msgid "        %s {-Q --query}   [options] [<package(s)>]"
msgstr "        %s {-Q --query}   [opzioni] [<pacchetti>]"

#: srcpac:41
msgid "        %s {-R --remove}  [options] <package(s)>"
msgstr "        %s {-R --remove}  [opzioni] <pacchetti>"

#: srcpac:42
msgid "        %s {-S --sync}    [options] [package(s)]"
msgstr "        %s {-S --sync}    [opzioni] [pacchetti]"

#: srcpac:43
msgid "%s options are based on pacman, so check the pacman man page"
msgstr "Le opzioni di %s sono basate su pacman, vedi il manuale di pacman"

#: srcpac:44
msgid "%s adds some option to -S:"
msgstr "%s aggiunge qualche opzione a -S:"

#: srcpac:45
msgid "  -b, --build    builds the targets from source"
msgstr "  -b, --build    compila tutti i pacchetti da sorgenti"

#: srcpac:46
msgid "  -bb            builds targets and their dependencies from source"
msgstr "  -bb           compila i pacchetti e le loro dipendenze da sorgenti"

#: srcpac:47
msgid ""
"  -bbb           builds targets, dependencies and their makedeps from source"
msgstr ""
"  -bbb           compila i pacchetti, le loro dipendenze e le dipendenze di "
"compilazione da sorgenti"

#: srcpac:48
msgid "  -m, --makedeps remove the makedepends after the build"
msgstr ""
"  -m, --makedeps rimuove le dipendenze di compilazione dopo la compilazione"

#: srcpac:49
msgid ""
"  -o, --onlyconf applies the changes and displays the PKGBUILD without "
"building"
msgstr ""
"  -o, --onlyconf applica i cambiamenti e visualizza il PKGBUILD senza "
"compilare"

#: srcpac:50
msgid "%s adds some option to -Q:"
msgstr "%s aggiunge qualche opzione a -Q:"

#: srcpac:51
msgid "  -b, --build    show all packages built from source"
msgstr "  -b, --build     visualizza tutti i pacchetti compilati da sorgenti"

#: srcpac:52
msgid "%s adds some option to -R:"
msgstr "%s aggiunge qualche opzione a -R:"

#: srcpac:53
msgid "  -b, --build    remove build-flag only, not the package"
msgstr ""
"  -b, --build    rimuove solo il flag di pacchetto compilato, non il "
"pacchetto"

#: srcpac:54
msgid "  -o, --onlyconf remove the patchfile only"
msgstr "  -o, --onlyconf rimuove solo il patchfile"

#: srcpac:139
msgid "Error: Could not find %s under %s"
msgstr "Errore: Impossibile trovare %s in %s"

#: srcpac:148
msgid "Starting ABS sync..."
msgstr "Inizio la sincronizzazione di ABS..."

#: srcpac:150 srcpac:406
msgid "done"
msgstr "fatto"

#: srcpac:404
msgid "removing source reference %s..."
msgstr "rimuovo il riferimento al pacchetto %s..."

#: srcpac:434
msgid "Error: You need to use sudo or to be root"
msgstr "Errore: Devi usare sudo oppure devi essere root"

#: srcpac:439
msgid "Error: The ABSROOT environment variable is not defined"
msgstr "Errore: La variabile d'ambiente ABSROOT non è stata definita"

#: srcpac:481
msgid "Source         : "
msgstr "Sorgenti         : "

#: srcpac:483 srcpac:489
msgid "Yes"
msgstr "Si"

#: srcpac:485 srcpac:491
msgid "No"
msgstr "No"

#: srcpac:487
msgid "Patchfile      : "
msgstr "Patchfile      : "

#: srcpac:496
msgid " [source]"
msgstr " [sorgenti]"

#: srcpac:580
msgid "Source Targets:"
msgstr "Pacchetti:"

#: srcpac:586
msgid "Proceed? [Y/n]"
msgstr "Continuo? [S/n]"

#: srcpac:588
msgid "y"
msgstr "s"

#: srcpac:588
msgid "Y"
msgstr "S"

#: srcpac:599
msgid "Build failed for:%s"
msgstr "La compilazione è fallita per:%s"


