#: srcpac:36
msgid "usage: %s <operation> [...]"
msgstr "upotreba: %s <operacija> [...]"

#: srcpac:37
msgid "operations:"
msgstr "operacije:"

#: srcpac:38
msgid "        %s {-h --help}"
msgstr "        %s {-h --help}"

#: srcpac:39
msgid "        %s {-V --version}"
msgstr "        %s {-V --version}"

#: srcpac:40
msgid "        %s {-Q --query}   [options] [<package(s)>]"
msgstr "        %s {-Q --query}   [opcije] [<paket(i)>]"

#: srcpac:41
msgid "        %s {-R --remove}  [options] <package(s)>"
msgstr "        %s {-R --remove}  [opcije] <paket(i)>"

#: srcpac:42
msgid "        %s {-S --sync}    [options] [package(s)]"
msgstr "        %s {-S --sync}    [opcije] [paket(i)]"

#: srcpac:43
msgid "%s options are based on pacman, so check the pacman man page"
msgstr ""
"opcije %sa su zasnovane na pacmanu, zato proveri pacmanovu man stranicu"

#: srcpac:44
msgid "%s adds some option to -S:"
msgstr "%s dodaje neke opcije na -S:"

#: srcpac:45
msgid "  -b, --build    builds the targets from source"
msgstr "  -b, --build    izgrađuje pakete iz izvornog koda"

#: srcpac:46
msgid "  -bb            builds targets and their dependencies from source"
msgstr ""
"  -bb            izgrađuje pakete i njihove prateće pakete iz izvornog koda"

#: srcpac:47
msgid ""
"  -bbb           builds targets, dependencies and their makedeps from source"
msgstr ""
"  -bbb           izgrađuje pakete i njihove prateće i gradeće pakete iz "
"izvornog koda"

#: srcpac:48
msgid "  -m, --makedeps remove the makedepends after the build"
msgstr "  -m, --makedeps ukloni gradeće pakete posle izgradnje"

#: srcpac:49
msgid ""
"  -o, --onlyconf applies the changes and displays the PKGBUILD without "
"building"
msgstr ""
"  -o, --onlyconf primenjuje promene i prikazuje PKGBUILD bez samog "
"izgrađivanja"

#: srcpac:50
msgid "%s adds some option to -Q:"
msgstr "%s dodaje neke opcije na -Q:"

#: srcpac:51
msgid "  -b, --build    show all packages built from source"
msgstr "  -b, --build    prikaži sve pakete izgrađene iz izvornog koda"

#: srcpac:52
msgid "%s adds some option to -R:"
msgstr "%s dodaje neke opcije na -R:"

#: srcpac:53
msgid "  -b, --build    remove build-flag only, not the package"
msgstr "  -b, --build    samo ukloni obeležje izgradnje, ne i paket"

#: srcpac:54
msgid "  -o, --onlyconf remove the patchfile only"
msgstr "  -o, --onlyconf ukloni samo zakrpni fajl"

#: srcpac:139
msgid "Error: Could not find %s under %s"
msgstr "Greška: Ne mogu da pronađem %s pod %s"

#: srcpac:148
msgid "Starting ABS sync..."
msgstr "Započinjem ABS usklađivanje..."

#: srcpac:150 srcpac:406
msgid "done"
msgstr "izvršeno"

#: srcpac:404
msgid "removing source reference %s..."
msgstr "%s: uklanjam napomenu na izvorni kod..."

#: srcpac:434
msgid "Error: You need to use sudo or to be root"
msgstr "Greška: Moraš da koristiš sudo ili da budeš root"

#: srcpac:439
msgid "Error: The ABSROOT environment variable is not defined"
msgstr "Greška: ABSROOT globalna promenljiva nije definisana"

#: srcpac:481
msgid "Source         : "
msgstr "Izvorni kod         : "

#: srcpac:483 srcpac:489
msgid "Yes"
msgstr "Da"

#: srcpac:485 srcpac:491
msgid "No"
msgstr "Ne"

#: srcpac:487
msgid "Patchfile      : "
msgstr "Zakrpni fajl      : "

#: srcpac:496
msgid " [source]"
msgstr " [izvorni kod]"

#: srcpac:580
msgid "Source Targets:"
msgstr "Paketi Za Izgradnju:"

#: srcpac:586
msgid "Proceed? [Y/n]"
msgstr "Nastavi? [D/n]"

#: srcpac:588
msgid "y"
msgstr "d"

#: srcpac:588
msgid "Y"
msgstr "D"

#: srcpac:599
msgid "Build failed for:%s"
msgstr "Izgradnja nije uspela za:%s"


